  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "node server.ts",
    "preview": "vite preview",
    "start": "npm run build && npm run serve",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "clean": "rm node_modules .next dist",
    "clean:cache": "npm cache clean --force",
    "setup": "npm run clean && npm run clean:cache && npm run fix-deps",
    "db:dump": "supabase db dump -f src/db/schema.sql",
    "db:pull": "supabase db pull",
    "db:push": "supabase db push",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "backup-schema": "node scripts/backup-schema.js",
    "download-models": "node scripts/download-models.js",
    "setup-fish-speech": "node scripts/setup-fish-speech.js",
    "insert-data": "tsx src/scripts/insert-pp1-data.ts",
    "rollback-dup": "cross-env NODE_NO_WARNINGS=1 tsx src/scripts/rollback-duplicates.ts",
    "memory-bank": "node scripts/memory-bank-manager.js",
    "remember": "node scripts/remember.js",
    "update-memory": "node scripts/update-memory-bank.js",
    "auto-update-memory": "node scripts/auto-update-memory.js",
    "install-hooks": "node scripts/install-hooks.js",
    "setup-scheduled-updates": "node scripts/setup-scheduled-updates.js",
    "update-file-tree": "node scripts/update-file-tree.js",
    "install-file-tree-hook": "node scripts/update-file-tree.js --install-hook",
    "log-chat": "node scripts/log-user-chat.js",
    "check-deps": "node scripts/check-dependencies.js",
    "fix-deps": "node scripts/fix-dependencies.js",
    "fix-deps-aggressive": "node scripts/fix-dependencies-aggressive.js",
    "add-dep": "node scripts/add-dependency.js",
    "fix-vulnerabilities": "node scripts/fix-vulnerabilities.js",
    "fix-vulnerabilities-simple": "node scripts/fix-vulnerabilities-simple.js",
    "remove-vulnerable-packages": "node scripts/remove-vulnerable-packages.js",
    "seed": "tsx --tsconfig tsconfig.seed.json src/db/seed/index.ts",
    "seed:dev": "cross-env NODE_ENV=development tsx --tsconfig tsconfig.seed.json src/db/seed/index.ts",
    "seed:test": "cross-env NODE_ENV=test tsx --tsconfig tsconfig.seed.json src/db/seed/index.ts",
    "migrate": "tsx src/db/migrate.ts",
    "migrate:dev": "cross-env NODE_ENV=development tsx src/db/migrate.ts",
    "migrate:test": "cross-env NODE_ENV=test tsx src/db/migrate.ts",
    "supabase:migrate": "supabase db push",
    "supabase:reset": "supabase db reset",
    "create-tables": "tsx src/db/create-tables.ts",
    "create-tables:dev": "cross-env NODE_ENV=development tsx src/db/create-tables.ts"
  },
