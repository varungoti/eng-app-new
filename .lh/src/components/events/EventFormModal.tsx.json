            "name": "Commit-0",
            "content": "import React from 'react';\r\nimport { X } from 'lucide-react';\r\nimport type { Event } from './EventCard';\r\nimport { Dialog, DialogContent, DialogHeader, DialogTitle, DialogDescription } from '../ui/dialog';\r\n\r\ninterface EventFormModalProps {\r\n  event?: Event;\r\n  onClose: () => void;\r\n  onSubmit: (event: Omit<Event, 'id'>) => void;\r\n}\r\n\r\nconst EventFormModal: React.FC<EventFormModalProps> = ({\r\n  event,\r\n  onClose,\r\n  onSubmit,\r\n}) => {\r\n  const handleSubmit = (e: React.FormEvent<HTMLFormElement>) => {\r\n    e.preventDefault();\r\n    const formData = new FormData(e.currentTarget);\r\n    \r\n    onSubmit({\r\n      title: formData.get('title') as string,\r\n      description: formData.get('description') as string,\r\n      startDate: formData.get('startDate') as string,\r\n      endDate: formData.get('endDate') as string,\r\n      location: formData.get('location') as string,\r\n      attendees: parseInt(formData.get('attendees') as string),\r\n      type: formData.get('type') as Event['type'],\r\n      status: formData.get('status') as Event['status'],\r\n    });\r\n  };\r\n\r\n  return (\r\n    <Dialog open onOpenChange={onClose}>\r\n      <DialogContent>\r\n        <DialogHeader className=\"border-b pb-4\">\r\n          <DialogTitle>\r\n            {event ? 'Edit Event' : 'Add New Event'}\r\n          </DialogTitle>\r\n          <DialogDescription>\r\n            Fill in the details below to {event ? 'update the' : 'create a new'} event.\r\n          </DialogDescription>\r\n        </DialogHeader>\r\n\r\n        <form onSubmit={handleSubmit} className=\"space-y-4 pt-4\">\r\n          <div>\r\n            <label className=\"block text-sm font-medium text-gray-700\">\r\n              Title\r\n            </label>\r\n            <input\r\n              type=\"text\"\r\n              name=\"title\"\r\n              defaultValue={event?.title}\r\n              required\r\n              className=\"mt-1 block w-full rounded-md border-gray-300 shadow-sm focus:border-indigo-500 focus:ring-indigo-500 text-sm\"\r\n              placeholder=\"Enter event title\"\r\n            />\r\n          </div>\r\n\r\n          <div>\r\n            <label className=\"block text-sm font-medium text-gray-700\">\r\n              Description\r\n            </label>\r\n            <textarea\r\n              name=\"description\"\r\n              defaultValue={event?.description}\r\n              rows={3}\r\n              className=\"mt-1 block w-full rounded-md border-gray-300 shadow-sm focus:border-indigo-500 focus:ring-indigo-500 text-sm\"\r\n              placeholder=\"Enter event description\"\r\n            />\r\n          </div>\r\n\r\n          <div className=\"grid grid-cols-2 gap-4\">\r\n            <div>\r\n              <label className=\"block text-sm font-medium text-gray-700\">\r\n                Start Date\r\n              </label>\r\n              <input\r\n                type=\"datetime-local\"\r\n                name=\"startDate\"\r\n                aria-label=\"Event start date and time\"\r\n                defaultValue={event?.startDate}\r\n                required\r\n                className=\"mt-1 block w-full rounded-md border-gray-300 shadow-sm focus:border-indigo-500 focus:ring-indigo-500 text-sm\"\r\n              />\r\n            </div>\r\n            <div>\r\n              <label className=\"block text-sm font-medium text-gray-700\">\r\n                End Date\r\n              </label>\r\n              <input\r\n                type=\"datetime-local\"\r\n                name=\"endDate\"\r\n                aria-label=\"Event end date and time\"\r\n                defaultValue={event?.endDate}\r\n                required\r\n                className=\"mt-1 block w-full rounded-md border-gray-300 shadow-sm focus:border-indigo-500 focus:ring-indigo-500 text-sm\"\r\n\r\n\r\n              />\r\n            </div>\r\n          </div>\r\n\r\n          <div>\r\n            <label className=\"block text-sm font-medium text-gray-700\">\r\n              Location\r\n            </label>\r\n            <input\r\n              type=\"text\"\r\n              name=\"location\"\r\n              defaultValue={event?.location}\r\n              required\r\n              className=\"mt-1 block w-full rounded-md border-gray-300 shadow-sm focus:border-indigo-500 focus:ring-indigo-500 text-sm\"\r\n              placeholder=\"Enter event location\"\r\n            />\r\n          </div>\r\n\r\n          <div>\r\n            <label className=\"block text-sm font-medium text-gray-700\">\r\n              Expected Attendees\r\n            </label>\r\n            <input\r\n              type=\"number\"\r\n              name=\"attendees\"\r\n              aria-label=\"Expected number of attendees\"\r\n              defaultValue={event?.attendees}\r\n              required\r\n              min=\"0\"\r\n              className=\"mt-1 block w-full rounded-md border-gray-300 shadow-sm focus:border-indigo-500 focus:ring-indigo-500 text-sm\"\r\n            />\r\n          </div>\r\n\r\n          <div className=\"grid grid-cols-2 gap-4\">\r\n            <div>\r\n              <label className=\"block text-sm font-medium text-gray-700\">\r\n                Type\r\n              </label>\r\n              <select\r\n                name=\"type\"\r\n                aria-label=\"Event type\"\r\n                defaultValue={event?.type}\r\n                required\r\n                className=\"mt-1 block w-full rounded-md border-gray-300 shadow-sm focus:border-indigo-500 focus:ring-indigo-500 text-sm\"\r\n\r\n              >\r\n                <option value=\"academic\">Academic</option>\r\n                <option value=\"administrative\">Administrative</option>\r\n                <option value=\"training\">Training</option>\r\n                <option value=\"meeting\">Meeting</option>\r\n                <option value=\"other\">Other</option>\r\n              </select>\r\n            </div>\r\n            <div>\r\n              <label className=\"block text-sm font-medium text-gray-700\">\r\n                Status\r\n              </label>\r\n              <select\r\n                name=\"status\"\r\n                aria-label=\"Event status\"\r\n                defaultValue={event?.status}\r\n                required\r\n                className=\"mt-1 block w-full rounded-md border-gray-300 shadow-sm focus:border-indigo-500 focus:ring-indigo-500 text-sm\"\r\n\r\n              >\r\n                <option value=\"upcoming\">Upcoming</option>\r\n                <option value=\"ongoing\">Ongoing</option>\r\n                <option value=\"completed\">Completed</option>\r\n                <option value=\"cancelled\">Cancelled</option>\r\n              </select>\r\n            </div>\r\n          </div>\r\n\r\n          <div className=\"mt-6 flex justify-end space-x-3 pt-4 border-t\">\r\n            <button\r\n              type=\"button\"\r\n              onClick={onClose}\r\n              className=\"px-4 py-2 border border-gray-300 rounded-md text-sm font-medium text-gray-700 hover:bg-gray-50 transition-colors\"\r\n            >\r\n              Cancel\r\n            </button>\r\n            <button\r\n            \r\n              type=\"submit\"\r\n              className=\"px-4 py-2 border border-transparent rounded-md shadow-sm text-sm font-medium text-white bg-indigo-600 hover:bg-indigo-700 transition-colors\"\r\n            >\r\n              {event ? 'Update Event' : 'Add Event'}\r\n            </button>\r\n          </div>\r\n        </form>\r\n      </DialogContent>\r\n    </Dialog>\r\n  );\r\n};\r\n\r\nexport default EventFormModal;"
        }
    ]
