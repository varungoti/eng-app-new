            "content": "import { Card, CardContent, CardHeader, CardTitle } from \"@/components/ui/card\";\r\nimport { Badge } from \"@/components/ui/badge\";\r\nimport { Button } from \"@/components/ui/button\";\r\nimport { Trash, Clock, MessageSquare } from \"lucide-react\";\r\n\r\nconst ContentSection = () => {\r\n  return (\r\n    <div className=\"space-y-6\">\r\n      {topics.map((topic) => (\r\n        <Card key={topic.id} className=\"border-l-4 border-l-primary/40 hover:border-l-primary transition-all\">\r\n          <CardHeader>\r\n            <div className=\"flex items-center justify-between\">\r\n              <div className=\"flex items-center gap-2\">\r\n                <CardTitle>{topic.title}</CardTitle>\r\n                <Badge variant=\"outline\">\r\n                  {subtopics.filter(s => s.topic_id === topic.id).length} Subtopics\r\n                </Badge>\r\n              </div>\r\n              <AlertDialog>\r\n                <AlertDialogTrigger asChild>\r\n                  <Button variant=\"ghost\" size=\"sm\" className=\"hover:bg-destructive/10\">\r\n                    <Trash className=\"h-4 w-4\" />\r\n                  </Button>\r\n                </AlertDialogTrigger>\r\n                <AlertDialogContent>\r\n                  <AlertDialogHeader>\r\n                    <AlertDialogTitle>Delete Topic</AlertDialogTitle>\r\n                    <AlertDialogDescription>\r\n                      This will permanently delete {topic.title} and all its content. This action cannot be undone.\r\n                    </AlertDialogDescription>\r\n                  </AlertDialogHeader>\r\n                  <AlertDialogFooter>\r\n                    <AlertDialogCancel>Cancel</AlertDialogCancel>\r\n                    <AlertDialogAction onClick={() => handleDelete(topic.id, 'topic')}>\r\n                      Delete\r\n                    </AlertDialogAction>\r\n                  </AlertDialogFooter>\r\n                </AlertDialogContent>\r\n              </AlertDialog>\r\n            </div>\r\n          </CardHeader>\r\n          <CardContent>\r\n            <div className=\"pl-4 space-y-4\">\r\n              {subtopics\r\n                .filter(subtopic => subtopic.topic_id === topic.id)\r\n                .map(subtopic => (\r\n                  <Card key={subtopic.id} className=\"border-l-2 border-l-muted hover:border-l-primary/60\">\r\n                    <CardHeader>\r\n                      <div className=\"flex items-center justify-between\">\r\n                        <div className=\"flex items-center gap-2\">\r\n                          <h3 className=\"font-medium\">{subtopic.title}</h3>\r\n                          <Badge variant=\"outline\">\r\n                            {lessons.filter(l => l.subtopic_id === subtopic.id).length} Lessons\r\n                          </Badge>\r\n                        </div>\r\n                        <AlertDialog>\r\n                          <AlertDialogTrigger asChild>\r\n                            <Button variant=\"ghost\" size=\"sm\">\r\n                              <Trash className=\"h-4 w-4\" />\r\n                            </Button>\r\n                          </AlertDialogTrigger>\r\n                          {/* Alert Dialog Content */}\r\n                        </AlertDialog>\r\n                      </div>\r\n                    </CardHeader>\r\n                    <CardContent>\r\n                      <div className=\"pl-4 space-y-2\">\r\n                        {lessons\r\n                          .filter(lesson => lesson.subtopic_id === subtopic.id)\r\n                          .map(lesson => (\r\n                            <div key={lesson.id} className=\"flex items-center justify-between p-2 rounded-md hover:bg-accent\">\r\n                              <div className=\"flex items-center gap-4\">\r\n                                <div className=\"flex flex-col\">\r\n                                  <span className=\"font-medium\">{lesson.title}</span>\r\n                                  <div className=\"flex items-center gap-2 text-xs text-muted-foreground\">\r\n                                    <div className=\"flex items-center\">\r\n                                      <Clock className=\"h-3 w-3 mr-1\" />\r\n                                      {lesson.duration || 0} mins\r\n                                    </div>\r\n                                    <div className=\"flex items-center\">\r\n                                      <MessageSquare className=\"h-3 w-3 mr-1\" />\r\n                                      {lesson.questions?.length || 0} Questions\r\n                                    </div>\r\n                                  </div>\r\n                                </div>\r\n                              </div>\r\n                              <AlertDialog>\r\n                                <AlertDialogTrigger asChild>\r\n                                  <Button variant=\"ghost\" size=\"sm\">\r\n                                    <Trash className=\"h-4 w-4\" />\r\n                                  </Button>\r\n                                </AlertDialogTrigger>\r\n                                {/* Alert Dialog Content */}\r\n                              </AlertDialog>\r\n                            </div>\r\n                          ))}\r\n                      </div>\r\n                    </CardContent>\r\n                  </Card>\r\n                ))}\r\n            </div>\r\n          </CardContent>\r\n        </Card>\r\n      ))}\r\n    </div>\r\n  );\r\n}; "
        }
    ]
